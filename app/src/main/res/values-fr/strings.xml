    <!--  Tutorial slides 0 -->
    <string name="tut0_title">Tutoriel</string>
    <string name="tut0_desc">Passons en revue les règles de base</string>

    <!--  Tutorial slides 1 -->
    <string name="tut1_title">Le plateau</string>
    <string name="tut1_desc">Les joueurs jouent à tour de rôle dans neuf grilles de tic tac toe, disposées à l\'intérieur d\'une plus grande grille</string>

    <!--  Tutorial slides 2 -->
    <string name="tut2_title">Gagner des grilles</string>
    <string name="tut2_desc">Gagnez des grilles en alignant votre signe trois fois, comme vous le feriez avec un tic tac toe ordinaire</string>

    <!--  Tutorial slides 3 -->
    <string name="tut3_title">Comment gagner</string>
    <string name="tut3_desc">Gagnez 3 grilles d\'affilée pour gagner la partie</string>

    <!--  Tutorial slides 4 -->
    <string name="tut4_title">Les couleurs</string>
    <string name="tut4_desc">Les coups possibles pour le joueur suivant sont indiqués dans la couleur de ce joueur. Le dernier coup est légèrement plus clair</string>

    <!--  Tutorial slides 5 -->
    <string name="tut5_title">Mouvements autorisés</string>
    <string name="tut5_desc">Vous décidez où l\'adversaire peut jouer ensuite. Exemple: Si vous jouez en haut à droite dans une grille, l\'adversaire doit jouer dans la grille en haut à droite</string>

    <!--  Tutorial slides 6 -->
    <string name="tut6_title">Coups libres</string>
    <string name="tut6_desc">Si vous êtes envoyé sur une grille complète ou gagnée, vous pouvez jouer où vous voulez</string>

    <!--  Tutorial slides 7 -->
    <string name="tut7_title">Mode de jeu</string>
    <string name="tut7_desc">Défiez vos amis en local ou en ligne. Jouez contre l’ordinateur et choisissez la difficulté</string>
    <string name="tut7_against">Versus</string>
    <string name="tut7_ways_computer">Ordinateur</string>
    <string name="tut7_ways_local">Ami local</string>
